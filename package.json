{
  "name": "adno-react",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "yarn install && parcel public/index.html",
    "build-all": "yarn build-full && yarn build-light",
    "build-full": "ADNO_MODE=FULL parcel build public/index.html --dist-dir adno-full",
    "build-light": "ADNO_MODE=LIGHT parcel build public/index.html --dist-dir adno-light",
    "clean": "rm -r node_modules && rm -r .parcel-cache && rm -r dist",
    "build-full-with-stats": "ADNO_MODE=FULL MATOMO_URL=https://stats.emf.fr/ MATOMO_SITE_ID=6 yarn install && parcel build public/index.html --dist-dir adno-full",
    "build-light-with-stats": "ADNO_MODE=LIGHT MATOMO_URL=https://stats.emf.fr/ MATOMO_SITE_ID=7 yarn install && parcel build public/index.html --dist-dir adno-light"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@toast-ui/editor": "^3.2.1",
    "@toast-ui/react-editor": "3.2.1",
    "autoprefixer": "^10.4.13",
    "daisyui": "^2.42.1",
    "editorjs-html": "^3.4.2",
    "editorjs-paragraph-with-alignment": "^3.0.0",
    "history": "^5.3.0",
    "popper.js": "^1.15.0",
    "postcss": "^8.4.19",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-html-parser": "^2.0.2",
    "react-markdown": "8.0.5",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-tag-input-component": "^2.0.2",
    "sweetalert2": "^11.4.29",
    "tailwindcss": "3.2.4",
    "turndown": "^7.1.1",
    "wikidata-sdk": "^8.1.0"
  },
  "devDependencies": {
    "buffer": "^5.5.0",
    "events": "^3.3.0",
    "parcel": "^2.7.0",
    "path-browserify": "^1.0.0",
    "process": "^0.11.10"
  }
}
